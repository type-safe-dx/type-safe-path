{
  "name": "type-safe-path",
  "version": "0.0.0",
  "license": "MIT",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "bin": "dist/cli.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "prepare": "husky install",
    "build": "unbuild",
    "lint:check": "rome check .",
    "lint:fix": "rome format --write .",
    "test": "vitest",
    "test:typecheck": "vitest typecheck",
    "test:coverage": "vitest --run --coverage"
  },
  "lint-staged": {
    "*.{ts}": [
      "rome format --write"
    ]
  },
  "dependencies": {
    "jiti": "1.16.2",
    "kleur": "4.1.5",
    "sade": "1.8.1",
    "tiny-glob": "0.2.9"
  },
  "devDependencies": {
    "@types/node": "18.6.4",
    "c8": "7.12.0",
    "husky": "^8.0.0",
    "lint-staged": "13.1.0",
    "rome": "11.0.0",
    "tsx": "3.12.2",
    "typescript": "4.7.4",
    "unbuild": "1.1.1",
    "vitest": "0.27.1"
  },
  "packageManager": "pnpm@7.8.0"
}
